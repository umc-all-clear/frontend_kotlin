<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:cardCornerRadius="@dimen/menu_radius">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/menu">
        <ImageView
            android:id="@+id/home_friend_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/home_friend_tv"
            app:layout_constraintBottom_toTopOf="@id/home_friend_cv"
            android:src="@drawable/ic_launcher_background"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="@dimen/calendar_des_side_margin"
            android:layout_marginBottom="40dp">
        </ImageView>
        <TextView
            android:id="@+id/home_friend_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/home_friend_iv"
            app:layout_constraintBottom_toTopOf="@id/home_friend_cv"
            android:text="1월의 1등"
            android:textSize="@dimen/home_cal_else"
            android:layout_marginTop="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="40dp">
        </TextView>

        <androidx.cardview.widget.CardView
        android:id="@+id/home_friend_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/home_friend_iv"
        app:layout_constraintBottom_toTopOf="@id/home_friend_more_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:cardCornerRadius="60dp"
            android:layout_marginLeft="@dimen/calendar_des_side_margin"
            android:layout_marginRight="@dimen/calendar_des_side_margin">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/home_friend_cl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:layout_margin="20dp">
            <androidx.cardview.widget.CardView
                android:id="@+id/home_friend_profile_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/home_friend_mail_tv">
                <ImageView
                    android:id="@+id/home_friend_profile_iv"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_launcher_background">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/home_friend_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/home_friend_profile_cv"
                app:layout_constraintBottom_toTopOf="@id/home_friend_mail_tv"
                android:text="name">
            </TextView>
            <TextView
                android:id="@+id/home_friend_mail_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/home_friend_name_tv"
                app:layout_constraintLeft_toRightOf="@id/home_friend_profile_cv"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="name@gmail.com">
            </TextView>
            <TextView
                android:id="@+id/home_friend_rate_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintRight_toLeftOf="@id/home_friend_rate_iv"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginRight="20dp"
                android:text="0.0">
            </TextView>
            <ImageView
                android:id="@+id/home_friend_rate_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:src="@drawable/ic_launcher_background">
            </ImageView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/home_friend_more_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/home_friend_cv"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:text="터치하여 친구 더보기 >"
                android:textSize="15dp"
                android:layout_marginRight="@dimen/calendar_des_side_margin">
            </TextView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>