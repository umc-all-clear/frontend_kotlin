<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="height"
            type="com.umc.clear.ui.home.adapter.HomeRVAdapter">
        </variable>
    </data>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_cal_cv"
    android:layout_width="match_parent"
    change_all_height="@{height.liveCvChange, default = true}"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/menu_radius"
    android:layout_marginBottom="@dimen/default_vertical_margin">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/home_cal_cl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/calendar_background">
        <TextView
            android:id="@+id/home_cal_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toTopOf="@id/home_cal_mon_tv"
            app:layout_constraintStart_toStartOf="@id/home_cal_mon_tv"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp"
            android:gravity="top"
            android:text="month"
            android:textSize="@dimen/home_cal_else">
        </TextView>
        <ImageView
            android:id="@+id/home_cal_prev_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/home_cal_next_iv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_mon_tv"
            android:src="@drawable/item_home_calendar_frame_prev"
            android:layout_marginTop="30dp"
            android:layout_marginRight="15dp">
        </ImageView>
        <ImageView
            android:id="@+id/home_cal_next_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintLeft_toRightOf="@id/home_cal_prev_iv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/home_cal_mon_tv"
            app:layout_constraintEnd_toEndOf="@id/home_cal_sun_tv"
            android:src="@drawable/item_home_calendar_frame_next"
            android:layout_marginTop="30dp">
        </ImageView>
        <TextView
            android:id="@+id/home_cal_mon_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/home_cal_tue_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            android:text="M"
            android:textSize="@dimen/home_cal_else"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            android:gravity="center">
        </TextView>
        <TextView
            android:id="@+id/home_cal_tue_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintLeft_toRightOf="@id/home_cal_mon_tv"
            app:layout_constraintRight_toLeftOf="@id/home_cal_wed_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            android:text="T"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <TextView
            android:id="@+id/home_cal_wed_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            app:layout_constraintLeft_toRightOf="@id/home_cal_tue_tv"
            app:layout_constraintRight_toLeftOf="@id/home_cal_thur_tv"
            android:text="W"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <TextView
            android:id="@+id/home_cal_thur_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            app:layout_constraintLeft_toRightOf="@id/home_cal_wed_tv"
            app:layout_constraintRight_toLeftOf="@id/home_cal_fri_tv"
            android:text="T"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <TextView
            android:id="@+id/home_cal_fri_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            app:layout_constraintLeft_toRightOf="@id/home_cal_thur_tv"
            app:layout_constraintRight_toLeftOf="@id/home_cal_sat_tv"
            android:text="F"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <TextView
            android:id="@+id/home_cal_sat_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            app:layout_constraintRight_toLeftOf="@id/home_cal_sun_tv"
            app:layout_constraintLeft_toRightOf="@id/home_cal_fri_tv"
            android:text="S"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <TextView
            android:id="@+id/home_cal_sun_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_tv"
            app:layout_constraintBottom_toTopOf="@id/home_cal_vp"
            app:layout_constraintLeft_toRightOf="@id/home_cal_sat_tv"
            app:layout_constraintRight_toRightOf="parent"
            android:text="S"
            android:textSize="@dimen/home_cal_else"
            android:gravity="center"
            android:layout_marginRight="25dp"
            app:layout_constraintHorizontal_weight="0.7">
        </TextView>
        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/home_cal_vp"
            change_cal_height="@{height.liveVpChange, default = true}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_cal_mon_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="30dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            android:orientation="horizontal"
            tools:listitem="@layout/item_home_calendar">
        </androidx.viewpager2.widget.ViewPager2>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
</layout>