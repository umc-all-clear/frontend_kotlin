<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
        <TextView
            android:id="@+id/dialog_setup_logout_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/dialog_setup_logout_cancel_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="로그아웃을 하시겠습니까?">
        </TextView>
    <TextView
        android:id="@+id/dialog_setup_logout_cancel_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/dialog_setup_logout_title_tv"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/dialog_setup_logout_confirm_tv"
        android:text="취소"
        app:layout_constraintHorizontal_chainStyle="packed">
    </TextView>
    <TextView
        android:id="@+id/dialog_setup_logout_confirm_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/dialog_setup_logout_title_tv"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/dialog_setup_logout_cancel_tv"
        app:layout_constraintRight_toRightOf="parent"
        android:text="로그아웃"
        app:layout_constraintHorizontal_chainStyle="packed">
    </TextView>
    </androidx.constraintlayout.widget.ConstraintLayout>