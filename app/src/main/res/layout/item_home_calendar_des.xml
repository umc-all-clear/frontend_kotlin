<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:id="@+id/item_cal_des_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item_cal_des_cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/item_cal_des_date_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintBottom_toTopOf="@id/item_cal_des_before_cv"
                android:text="date">
            </TextView>
            <ImageView
                android:id="@+id/item_calendar_rank_close_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toTopOf="@id/item_cal_des_before_tv">
            </ImageView>
            <TextView
                android:id="@+id/item_cal_des_before_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_cal_des_date_tv"
                app:layout_constraintBottom_toTopOf="@id/item_cal_des_before_cv"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent">
            </TextView>
            <androidx.cardview.widget.CardView
                android:id="@+id/item_cal_des_before_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/item_cal_des_before_tv"
                app:layout_constraintBottom_toTopOf="@id/item_cal_des_after_tv">
                <ImageView
                    android:id="@+id/item_cal_des_before_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/item_cal_des_after_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/item_cal_des_before_cv"
                app:layout_constraintBottom_toTopOf="@id/item_cal_des_after_cv">
            </TextView>
            <androidx.cardview.widget.CardView
                android:id="@+id/item_cal_des_after_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/item_cal_des_after_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/item_cal_des_com_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>
            <androidx.cardview.widget.CardView
                android:id="@+id/item_cal_des_com_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/cal_des_com_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </TextView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/item_cal_des_fed_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>
            <androidx.cardview.widget.CardView
                android:id="@+id/item_cal_des_fed_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/item_cal_fed_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </TextView>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>