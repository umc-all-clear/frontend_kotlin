<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/login_go_back_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </ImageView>
    <TextView
        android:id="@+id/login_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <TextView
        android:id="@+id/login_des_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <EditText
        android:id="@+id/login_id_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </EditText>
    <EditText
        android:id="@+id/login_pw_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </EditText>
    <CheckBox
        android:id="@+id/login_auto_login_cb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </CheckBox>
    <TextView
        android:id="@+id/login_auto_login_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <CheckBox
        android:id="@+id/login_id_save_cb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </CheckBox>
    <TextView
        android:id="@+id/login_id_save_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <TextView
        android:id="@+id/login_go_login_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <TextView
        android:id="@+id/login_go_signup_des_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <TextView
        android:id="@+id/login_go_signup_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <ImageView
        android:id="@+id/login_go_signup_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </ImageView>
</androidx.constraintlayout.widget.ConstraintLayout>